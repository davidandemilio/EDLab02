        </td>
    </tr>
}

    

</table>
<h4>Carga Json</h4>

@using (Html.BeginForm("CargaJsonEnt", "Entero", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form-group">
        <input type="file" name="archivo" />
        <input type="submit" value="Importar" />



    </div>


}
